{
  "name": "ImageManipulation",
  "version": "0.1.0",
  "description": "R&D of tracking JS library",
  "main": "index.js",
  "scripts": {
    "build:js:main:debug": "browserify -d src/main.js -o js/index.js",
    "build:js:main": "browserify src/main.js | uglifyjs > js/index.js",
    "build:js:debug": "npm run build:js:main:debug",
    "build:js": "npm run build:js:main",
    "build": "npm run build:js",
    "build:debug": "npm run build:js:debug",
    "build:watch": "watch 'npm run build' .",
    "build:watch:debug": "watch 'npm run build:debug' ."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claudiordgz/ImageManipulation"
  },
  "keywords": [
    "javascript",
    "face",
    "recognition",
    "color",
    "palette",
    "tracking",
    "js"
  ],
  "author": "claudiordgz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/claudiordgz/ImageManipulation/issues"
  },
  "homepage": "https://claudiordgz.github.io/ImageManipulation",
  "devDependencies": {
    "bootstrap": "3.3.5",
    "browserify": "11.0.1",
    "icanhaz": "0.10.3",
    "jquery": "2.1.4",
    "tracking": "1.1.1",
    "uglifyjs": "2.4.10",
    "watch": "^0.16.0"
  }
